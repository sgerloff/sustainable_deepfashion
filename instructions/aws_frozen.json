{
  "train_dataframe": "data/processed/category_id_1_deepfashion_train.joblib",
  "validation_dataframe": "data/processed/category_id_1_deepfashion_validation.joblib",
  "model": "EfficientNetTriplet",
  "load": "None",
  "learning_rate": 1e-5,
  "trainable_basemodel_ratio": 0.0,
  "train_keywords": {
    "epochs": 100,
    "training_ratio": 1.0,
    "batch_size": 32
  },
  "callbacks": {
    "Checkpoint": {
      "path": "effnet_frozen.h5"
    },
    "Tensorboard": {
      "log_dir": "effnet_frozen"
    },
    "CyclicLR": {
      "base_lr": 1e-5,
      "max_lr": 1e1,
      "step_size": 1824,
      "mode": "exp_range"
    }
  },
  "clean_script": "aws/cleanup.sh"
}
