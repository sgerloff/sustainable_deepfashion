{
  "copy_instruction": "simple_conv2d.json",
  "train_data": {
    "kwargs": {
      "batch_size": 32
    }
  },
  "model": {
    "kwargs": {
      "embedding_size": 16
    },
    "fit": {
      "kwargs": {
        "epochs": 200
      }
    }
  },
  "loss": {
    "loss": "tensorflow_addons.losses.TripletSemiHardLoss",
    "kwargs": {
      "distance_metric": "angular"
    }
  },
  "callbacks": {
    "src.models.callbacks.CyclicLR": {
      "base_lr": 1e-6,
      "max_lr": 1e-4,
      "step_size": 5184,
      "mode": "exp_range"
    }
  }
}