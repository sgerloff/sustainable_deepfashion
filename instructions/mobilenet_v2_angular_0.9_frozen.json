{
  "copy_instruction": "efficientnetb0_frozen.json",
  "train_data": {
    "kwargs": {
      "batch_size": 32
    }
  },
  "model": {
    "factory": "src.models.mobile_net_triplet_model_factory.MobileNetV2TripletModelFactory",
    "basemodel_freeze_ratio": 0.9,
    "kwargs": {
      "input_shape": "(224, 224, 3)",
      "embedding_size": 16,
      "extraction_layers_size": "[1024, 512, 256]"
    },
    "load": "None",
    "fit": {
      "kwargs": {
        "epochs": 50
      }
    }
  },
  "optimizer": {
    "optimizer": "tensorflow.keras.optimizers.Adam",
    "kwargs": {
      "learning_rate": 1e-5,
      "clipnorm": 0.5
    }
  },
  "loss": {
    "loss": "tensorflow_addons.losses.TripletSemiHardLoss",
    "kwargs": {
      "distance_metric": "angular"
    }
  },
  "callbacks": {
    "src.models.callbacks.CyclicLR": {
      "base_lr": 1e-5,
      "max_lr": 1e-3,
      "step_size": 37560,
      "mode": "exp_range"
    }
  }
}